# Ctrl + MouseWheel does nothing (for chrome)
""
  control + b:4 (mouse)

""
  control + b:5 (mouse)

# Shift + Ctrl + MouseWheel = volume
"mpc volume +2"
  shift + control + b:4 (mouse)

"mpc volume -2"
  shift + control + b:5 (mouse)

# lock
"slock"
  Mod4 + l

"slock"
  Print

# magic keys
"google-chrome-beta"
  XF86HomePage

"volume toggle"
  XF86AudioMute

"volume down"
  XF86AudioLowerVolume

"volume up"
  XF86AudioRaiseVolume
